@Article{Ji1993217,
  Title                    = {The nucleon structure functions from deep-inelastic scattering with electroweak currents },
  Author                   = {Xiangdon Ji},
  Journal                  = {Nuclear Physics B },
  Year                     = {1993},
  Number                   = {1–2},
  Pages                    = {217 - 250},
  Volume                   = {402},

  Abstract                 = {In deeply inelastic lepton-nucleon scattering in the standard model, we define 14 nucleon structure functions. We explain the physics content of these structure functions and study their Bjorken scaling and Regge behavior. We then systematically discuss twist-two, twist-three, and twist-four parton distributions of the nucleon defined through light-cone correlations. Using these, we calculate all the nucleon structure functions in perturbative QCD. Finally, we discuss some useful implications of our results. },
